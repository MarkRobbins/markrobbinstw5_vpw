.tc-tiddler-frame .top-bar-brain{display:none;}

.top-bar-brain{
  top:-35px;
  left:14px;
  xoverflow:hidden;
  width:calc(100vw - 120px);
  height:calc(100vh - 5px);
  xborder:1px solid green;
  position:absolute;
  margin-top:calc(-100vh + 35px);
  transition: 750ms;
  transition-delay: 1s;
  xopacity:0.7;
  
}
.top-bar-brain:hover{
  margin-top:0;
  transition:2500ms;
  box-shadow:0px 0px 14px 0px #339;
  transition-delay: 1s;
}
.top-bar-brain iframe{
  margin:0px;width:90vw;
  width:calc(100vw - 120px);
  height:calc(100vh - 10px);
  x-webkit-filter:invert(1) hue-rotate(180deg);
  x-filter:invert(1) hue-rotate(180deg);
}
.top-bar-brain:hover #thebrain-text,
#thebrain-text:hover{
  -webkit-filter: brightness(1);
  -webkit-transition : -webkit-filter 500ms linear;
}
#thebrain-text{
  opacity:0.6;
  display:block;
  xborder:1px solid blue;
  xmargin-top:-4px;
  -webkit-filter: brightness(0.5);
  -webkit-transition : -webkit-filter 500ms linear;
  xwidth:279px;
  height:20px;
  background-repeat:no-repeat;
  background-image: url('data:image/png;base64,');
}
