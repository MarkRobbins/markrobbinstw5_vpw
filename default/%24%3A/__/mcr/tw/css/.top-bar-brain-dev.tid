.tc-tiddler-frame .top-bar-brain-dev{display:none;}

.top-bar-brain-dev{
  top:-35px;
  left:14px;
  xoverflow:hidden;
  width:calc(100vw - 120px);
  height:calc(100vh - 5px);
  xborder:1px solid green;
  position:absolute;
  margin-top:calc(-100vh + 35px);
  transition: 750ms;
  transition-delay: 1s;
  opacity:0.7;
  
}
.top-bar-brain-dev:hover{
  margin-top:0;
  transition:2500ms;
  box-shadow:0px 0px 14px 0px #339;
  transition-delay: 1s;
}
.top-bar-brain-dev iframe{
  margin:0px;width:90vw;
  width:calc(100vw - 120px);
  height:calc(100vh - 10px);
  -webkit-filter:invert(1) hue-rotate(180deg);
  filter:invert(1) hue-rotate(180deg);  
}
.top-bar-brain-dev:hover #thebrain-btn,
#thebrain-btn:hover{
  -webkit-filter: brightness(1);
  -webkit-transition : -webkit-filter 500ms linear;
}

#thebrain-btn{
  opacity:0.6;
  display:block;
  -webkit-filter: brightness(0.5);
  -webkit-transition : -webkit-filter 500ms linear;
  margin-left:289px;
  width:28px;
  height:20px;
  background-repeat:no-repeat;
  background-image: url('data:image/png;base64,');
}
